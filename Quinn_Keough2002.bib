@article{Ward.Quinn1988,
  title = {{{PRELIMINARY INVESTIGATIONS OF THE ECOLOGY OF THE INTERTIDAL PREDATORY GASTROPOD LEPSIELLA VINOSA}} ({{LAMARCK}}) ({{GASTROPODA MURICIDAE}})},
  author = {WARD, S. and QUINN, G.P.},
  year = {1988},
  month = feb,
  journal = {Journal of Molluscan Studies},
  volume = {54},
  number = {1},
  pages = {109--117},
  issn = {0260-1230},
  doi = {10.1093/mollus/54.1.109},
  abstract = {Lepsiella vinosa\textbackslash{} (Lamarck), a common intertidal predatory gastropod, was studied in two zones on an exposed rocky shore in Victoria (Australia). Those from the Mussel Zone in the mid-shore primarily ate one species of mussel and had a faster rate of energy consumption than those from the Littorinid Zone in the high-shore, which mainly ate one species of littorinid gastropod. Shell length, growth rate and size-specific body weight of L. vinosa were all significantly greater in the Mussel Zone. Egg capsules of L. vinosa from the Mussel Zone contained significantly more eggs and more potential hatchlings per capsule than those from the Littorinid Zone although there were no significant differences in the sizes of eggs or hatchlings between zones. These differences between the two zones were correlated to the greater consumption of energy by L. vinosa in the Mussel Zone and were discussed in relation to the absence of dispersive larvae in this species},
  file = {/Users/tmartin/Zotero/storage/DLXWF77M/1023541.html}
}


